
<div class="col-lg-8">
   <br>
<h3>Users</h3>
<table class="table table-striped">
 <thead>
  <th>image</th>
  <th>name</th>
  <th>booklist</th>
 </thead>

 <tbody>
  <% @users.each do |user| %>
   <tr>
  <td><%= image_tag user.get_profile_image(100,100) %></td>
  <td><%= user.name %></td>
  <td><%= link_to 'Show', user_path(user.id)%></td>
   </tr>
  <% end %>
  </tbody>
 </table>
</div>



